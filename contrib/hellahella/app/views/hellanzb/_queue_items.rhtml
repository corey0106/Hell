<% @queue.each do |item| -%>
  <div id="nzb_<%= item["id"] %>" class="dragitem">
    <h4 class="handle"><%= item['nzbName'] %></h4>
  </div>
<% end -%>

<% if @message -%>
  <%= @message %>
<% end -%>
<%= sortable_element "nzb",
    :url => {:controller => 'live', :action=>'update_order'},
    :tag => 'div',
    :handle => 'handle',
    :containment => ['nzb'] %>